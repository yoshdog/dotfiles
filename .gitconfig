[user]
	name = Toan Nguyen
	email = toan@graduate.uwa.edu.au
[push]
	default = simple
[color]
	ui = true
[core]
	autocrlf = input
	safecrlf = false
[alias]
	co = checkout
	ci = commit -m
	st = status
	br = branch
	hist = log --pretty=format:'%h %ad | %s%d [%an]' --graph --date=short
	type = cat-file -t
	dump = cat-file -p
    h = log --pretty=format:'%Cblue%h%Creset %C(yellow)%d%Creset %s  %Cgreen%cr, %an' --abbrev-commit --date=relative --graph
