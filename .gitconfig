[user]
	name = Toan Nguyen
	email = toan@graduate.uwa.edu.au
[push]
	default = simple
[color]
	ui = true
[core]
	autocrlf = input
	safecrlf = true
[alias]
	co = checkout
	ci = commit -m
	st = status
	br = branch
	hist = log --pretty=format:'%h %ad | %s%d [%an]' --graph --date=short
	type = cat-file -t
	dump = cat-file -p
